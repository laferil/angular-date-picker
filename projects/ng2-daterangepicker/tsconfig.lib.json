{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"outDir": "../../out-tsc/lib",
		"target": "ES2019",
		"declaration": true,
		"inlineSources": true,
		"types": [],
		"lib": [
			"dom",
			"ES2019"
		]
	},
	"angularCompilerOptions": {
		"skipTemplateCodegen": true,
		"strictMetadataEmit": true,
		"enableResourceInlining": true
	},
	"exclude": [
		"src/test.ts",
		"**/*.spec.ts"
	]
}